
<div class="col-lg-8 post-list">
    <!-- Start latest-post Area -->

    <div class="latest-post-wrap">
        <h4 class="cat-title">Latest News</h4>
        {{#each datas}}
        <div class="single-latest-post row align-items-center">
            <div class="col-lg-5 post-left">
                <div class="feature-img relative">
                    <div class="overlay overlay-bg"></div>
                    <img class="img-fluid" src="{{this.images}}" alt="">
                </div>
                <ul class="tags">
                    <li><a href="#">Lifestyle</a></li>
                </ul>
            </div>
            <div class="col-lg-7 post-right">
                <a href="/post">
                    <h4>{{this.title}}</h4>
                </a>
                <ul class="meta">
                    <li><a href="#"><span class="lnr lnr-user"></span>{{this.user}}</a></li>
                    <li><a href="#"><span class="lnr lnr-calendar-full"></span>{{this.calendar}}</a></li>
                    <li><a href="#"><span class="lnr lnr-bubble"></span>{{this.comment}}</a></li>
                </ul>
                <p class="excert">
                    {{this.excert}}
                </p>
            </div>
        </div>
        {{/each}}
        <div class="load-more">
            <a href="#" class="primary-btn">Load More Posts</a>
        </div>
        
    </div>

    <!-- End latest-post Area -->
</div>
{{!-- sidebar --}}
<div class="col-lg-4">
    {{>sidebar}}
</div>

